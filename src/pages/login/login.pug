doctype html
html
    head
        title 金盾时代-综合业务系统
        include ./../../pug_include/head

        script(data-src='login.min.js?t='+tt)

        style.
            html,body{width:100vw;height:100vh;}
            .fullScreen{width: 100%; height: 100%;}
            .logo{padding:20px 0 40px 0;}
    body

        div(class='fullScreen box_scc')
            div(class='logo') 登录
            div(class='login_main openWin_main' id='form')
                b-input(id='userName' type='text' name='用户名' placeholder='请输入用户名' rule='must' err='请输入用户名')
                b-input(id='password' type='password' name='密码' placeholder='请输入密码' rule='must' err='请输入密码')



            div(class='openWin_btns box_hcc')
                div(class='openWin_btn_blue box_hcc hover' id='submit') 登录



