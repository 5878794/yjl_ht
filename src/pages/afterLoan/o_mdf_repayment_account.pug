doctype html
html
    head
        title 变更还款账号
        include ./../../pug_include/head_pc

        script(data-src='o_mdf_repayment_account.min.js?t='+tt)
        style.
            body,html{overflow:auto;}


    body

        div(class='openWin_main autoWidth')
            div
                b-title(name='变更还款账号')
                div(class='openWin_input_main box_hlt box_lines' id='form')
                    div(class='openWin_input_item1')
                        b-input(type='text' err='' name='还款账户' id='bankName')
                    div(class='openWin_input_item1')
                        b-input(type='text' err='' name='还款账号' id='bankNo')
                    div(class='openWin_input_item1')
                        b-input(type='text' err='' name='开户行' id='openBank')


            div(class='openWin_btns box_hcc')
                div(class='openWin_btn_gray box_hcc hover' id='cancel') 取消
                div(class='openWin_btn_blue box_hcc hover' id='submit') 提交
